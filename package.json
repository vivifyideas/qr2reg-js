{
  "name": "qr2reg-js",
  "version": "0.0.11",
  "description": "QR2REG JavaScript library",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn umd && yarn transpile && yarn clean",
    "umd": "rollup lib/index.js --file dist/umd.js --format umd --name QR2REGClient",
    "transpile": "babel dist/umd.js --out-file dist/index.js",
    "clean": "rimraf dist/umd.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vivifyideas/qr2reg-js"
  },
  "author": "QR2REG",
  "bugs": {
    "url": "https://github.com/vivifyideas/qr2reg-js/issues"
  },
  "homepage": "https://github.com/vivifyideas/qr2reg-js",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "rimraf": "^2.6.3",
    "rollup": "^1.10.0"
  }
}
